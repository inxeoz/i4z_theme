{
"name": "i4ztheme",
  "displayName": "i4z theme",
  "description": "A crisp light / dark theme with violet keywords, lime strings, blue types, and slate comments.",
  "version": "0.0.1",
  "publisher": "you",
  "engines": { "vscode": "^1.80.0" },
  "categories": ["Themes"],
  "contributes": {
    "themes": [

        {
        "label": "i4z_dark",
        "uiTheme": "vs",
        "path": "./themes/i4z_dark.json"
      },

      {
        "label": "i4z_light",
        "uiTheme": "vs",
        "path": "./themes/i4z_light.json"
      },

       {
        "label": "JD Dark",
        "uiTheme": "vs",
        "path": "./themes/JD_dark.json"
      },

          {
        "label": "JD Light",
        "uiTheme": "vs",
        "path": "./themes/JD_light.json"
      },

        {
        "label":  "i4z Light Warm",
        "uiTheme": "vs",
        "path": "./themes/i4z_light_Warm.json"
      }

       
    ],
    "configuration": {
      "properties": {
        "mytickLight.semanticHighlighting": {
          "type": "boolean",
          "default": true,
          "description": "Enable semantic token coloring (recommended)."
        }
      }
    }
  }
}
