{
"name": "i4ztheme",
  "displayName": "i4z theme",
  "description": "A crisp light / dark theme with violet keywords, lime strings, blue types, and slate comments.",
  "version": "0.0.1",
  "publisher": "you",
  "engines": { "vscode": "^1.80.0" },
  "categories": ["Themes"],
  "contributes": {
    "themes": [
      {
        "label": "Midnight Snack",
        "uiTheme": "vs-dark",
        "path": "./themes/Midnight-Snack.json"
      },
      {
        "label": "Unicorn Dreams",
        "uiTheme": "vs",
        "path": "./themes/Unicorn-Dreams.json"
      },
      {
        "label": "Coffee Overflow",
        "uiTheme": "vs-dark",
        "path": "./themes/Coffee-Overflow.json"
      },
      {
        "label": "Sunshine Overload",
        "uiTheme": "vs",
        "path": "./themes/Sunshine-Overload.json"
      },
      {
        "label": "Ninja Mode",
        "uiTheme": "vs-dark",
        "path": "./themes/Ninja-Mode.json"
      },
      {
        "label": "Rainbow Explosion",
        "uiTheme": "vs",
        "path": "./themes/Rainbow-Explosion.json"
      },

      {
        "label": "Mugshot",
        "uiTheme": "vs",
        "path": "./themes/Mugshot.json"
      }

       
    ],
    "configuration": {
      "properties": {
        "mytickLight.semanticHighlighting": {
          "type": "boolean",
          "default": true,
          "description": "Enable semantic token coloring (recommended)."
        }
      }
    }
  }
}
