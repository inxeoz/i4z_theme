{
  "$schema": "vscode://schemas/color-theme",
  "name": "JL Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "titleBar.activeBackground": "#f1f5f9",
    "titleBar.activeForeground": "#0f172a",
    "titleBar.inactiveBackground": "#e2e8f0",
    "titleBar.inactiveForeground": "#64748b",

    "commandCenter.background": "#f1f5f9",
    "commandCenter.foreground": "#0f172a",
    "commandCenter.activeBackground": "#e2e8f0",
    "commandCenter.activeForeground": "#0f172a",
    "commandCenter.border": "#cbd5e1",
    "commandCenter.inactiveForeground": "#94a3b8",

    "editorGroupHeader.tabsBackground": "#ffffff",
    "editorGroupHeader.noTabsBackground": "#ffffff",
    "editorGroupHeader.tabsBorder": "#e2e8f0",

    "tab.activeBackground": "#ffffff",
    "tab.activeForeground": "#0f172a",
    "tab.activeBorderTop": "#7c3aed",
    "tab.border": "#e2e8f0",
    "tab.unfocusedActiveBackground": "#ffffff",
    "tab.inactiveBackground": "#f8fafc",
    "tab.unfocusedInactiveBackground": "#f8fafc",
    "tab.inactiveForeground": "#64748b",
    "tab.hoverBackground": "#f1f5f9",

    "editor.background": "#ffffff",
    "editor.foreground": "#0f172a",
    "editor.lineHighlightBackground": "#f1f5f9",
    "editor.selectionBackground": "#c7d2fe77",
    "editor.inactiveSelectionBackground": "#e0e7ff88",
    "editorCursor.foreground": "#0f172a",
    "editorWhitespace.foreground": "#e2e8f0",
    "editorIndentGuide.background": "#e5e7eb",
    "editorIndentGuide.activeBackground": "#cbd5e1",
    "editorLineNumber.foreground": "#94a3b8",
    "editorLineNumber.activeForeground": "#0f172a",

    "editor.findMatchBackground": "#7c3aed55",
    "editor.findMatchBorder": "#7c3aed",
    "editor.findMatchHighlightBackground": "#7c3aed22",
    "editor.selectionHighlightBackground": "#7c3aed22",
    "editor.wordHighlightBackground": "#2563eb22",
    "editor.wordHighlightStrongBackground": "#2563eb33",
    "editorBracketMatch.background": "#e0e7ff77",
    "editorBracketMatch.border": "#7c3aed",

    "sideBar.background": "#f8fafc",
    "sideBar.foreground": "#0f172a",
    "sideBar.border": "#e2e8f0",
    "sideBarSectionHeader.background": "#e2e8f0",
    "sideBarSectionHeader.foreground": "#0f172a",

    "panel.background": "#ffffff",
    "panel.border": "#e2e8f0",
    "panelTitle.activeForeground": "#0f172a",
    "panelTitle.inactiveForeground": "#64748b",

    "list.focusBackground": "#eef2ff",
    "list.focusForeground": "#0f172a",
    "list.activeSelectionBackground": "#c7d2fe",
    "list.activeSelectionForeground": "#0f172a",
    "list.inactiveSelectionBackground": "#e0e7ff",
    "list.inactiveSelectionForeground": "#0f172a",
    "list.hoverBackground": "#f1f5f9",
    "list.hoverForeground": "#0f172a",
    "list.highlightForeground": "#0f172a",

    "quickInput.background": "#ffffff",
    "quickInput.foreground": "#0f172a",
    "pickerGroup.border": "#e2e8f0",
    "pickerGroup.foreground": "#64748b",

    "dropdown.background": "#f8fafc",
    "dropdown.foreground": "#0f172a",
    "dropdown.border": "#e2e8f0",

    "input.background": "#f8fafc",
    "input.foreground": "#0f172a",
    "input.border": "#e2e8f0",
    "input.placeholderForeground": "#94a3b8",

    "menu.background": "#ffffff",
    "menu.foreground": "#0f172a",
    "menu.separatorBackground": "#e2e8f0",
    "menubar.selectionBackground": "#e2e8f0",
    "menubar.selectionForeground": "#0f172a",

    "activityBar.background": "#f8fafc",
    "activityBar.foreground": "#0f172a",
    "activityBarBadge.background": "#7c3aed",
    "activityBarBadge.foreground": "#ffffff",

    "statusBar.background": "#f1f5f9",
    "statusBar.foreground": "#0f172a",
    "statusBar.border": "#e2e8f0",

    "textLink.foreground": "#2563eb",
    "textLink.activeForeground": "#1d4ed8",
    "focusBorder": "#7c3aed",
    "selection.background": "#c7d2feaa",

    "notification.background": "#f8fafc",
    "notification.foreground": "#0f172a",
    "notification.buttonBackground": "#7c3aed",
    "notification.buttonForeground": "#ffffff",
    "notification.infoBackground": "#dbeafe",
    "notification.warningBackground": "#fef9c3",
    "notification.errorBackground": "#fee2e2",

    "editorWidget.background": "#ffffff",
    "editorWidget.foreground": "#0f172a",
    "peekViewTitle.background": "#f8fafc",
    "peekViewEditor.background": "#ffffff",
    "peekViewResult.background": "#ffffff",
    "scrollbarSlider.background": "#cbd5e199",
    "scrollbarSlider.hoverBackground": "#94a3b8bb",
    "scrollbarSlider.activeBackground": "#64748bcc",

    "button.background": "#7c3aed",
    "button.hoverBackground": "#6d28d9",
    "button.foreground": "#ffffff",

    "badge.background": "#7c3aed",
    "badge.foreground": "#ffffff",

    "terminal.background": "#ffffff",
    "terminal.foreground": "#0f172a",
    "terminal.ansiBlack": "#1e293b",
    "terminal.ansiRed": "#b91c1c",
    "terminal.ansiGreen": "#15803d",
    "terminal.ansiYellow": "#a16207",
    "terminal.ansiBlue": "#1d4ed8",
    "terminal.ansiMagenta": "#9333ea",
    "terminal.ansiCyan": "#0891b2",
    "terminal.ansiWhite": "#334155",
    "terminal.ansiBrightBlack": "#64748b",
    "terminal.ansiBrightRed": "#ef4444",
    "terminal.ansiBrightGreen": "#22c55e",
    "terminal.ansiBrightYellow": "#eab308",
    "terminal.ansiBrightBlue": "#3b82f6",
    "terminal.ansiBrightMagenta": "#a855f7",
    "terminal.ansiBrightCyan": "#06b6d4",
    "terminal.ansiBrightWhite": "#0f172a",

    "gitDecoration.addedResourceForeground": "#15803d",
    "gitDecoration.modifiedResourceForeground": "#1d4ed8",
    "gitDecoration.deletedResourceForeground": "#dc2626",
    "gitDecoration.untrackedResourceForeground": "#15803d",
    "gitDecoration.ignoredResourceForeground": "#94a3b8",
    "gitDecoration.conflictingResourceForeground": "#a16207"
  },
  "tokenColors": [
    { "scope": ["comment"], "settings": { "foreground": "#94a3b8", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"], "settings": { "foreground": "#7c3aed", "fontStyle": "bold" } },
    { "scope": ["support.type", "entity.name.type", "entity.name.type.interface", "storage.type.type", "meta.type.annotation", "entity.name.namespace"], "settings": { "foreground": "#2563eb" } },
    { "scope": ["string", "punctuation.definition.string"], "settings": { "foreground": "#16a34a" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#db2777" } },
    { "scope": ["variable.other.constant", "support.constant", "constant.language"], "settings": { "foreground": "#0ea5e9" } },
    { "scope": ["entity.name.function", "support.function", "meta.function-call"], "settings": { "foreground": "#0f172a" } },
    { "scope": ["variable.parameter", "meta.parameter"], "settings": { "foreground": "#334155" } },
    { "scope": ["entity.name.type.interface.ts", "entity.other.inherited-class"], "settings": { "foreground": "#2563eb", "fontStyle": "bold" } },
    { "scope": ["entity.name.type.class", "meta.class", "support.class"], "settings": { "foreground": "#0ea5e9" } },
    { "scope": ["meta.decorator", "storage.type.annotation", "punctuation.decorator"], "settings": { "foreground": "#d97706" } },
    { "scope": ["entity.name.tag", "support.class.component", "meta.tag.tsx"], "settings": { "foreground": "#ea580c" } },
    { "scope": ["entity.other.attribute-name", "meta.tag.attributes.tsx"], "settings": { "foreground": "#0ea5e9" } }
  ],
  "semanticTokenColors": {
    "function": "#0f172a",
    "parameter": "#334155",
    "property": "#0f172a",
    "variable.readonly": "#0f172a",
    "type": "#2563eb",
    "class": "#0ea5e9",
    "enumMember": "#0ea5e9",
    "keyword": "#7c3aed",
    "string": "#16a34a",
    "number": "#db2777",
    "comment": "#94a3b8"
  }
}
