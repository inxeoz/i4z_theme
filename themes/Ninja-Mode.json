{
  "$schema": "vscode://schemas/color-theme",
  "name": "JD Dark",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "titleBar.activeBackground": "#111827",
    "titleBar.activeForeground": "#f8fafc",
    "titleBar.inactiveBackground": "#1e293b",
    "titleBar.inactiveForeground": "#94a3b8",

    "commandCenter.background": "#111827",
    "commandCenter.foreground": "#f8fafc",
    "commandCenter.activeBackground": "#1f2937",
    "commandCenter.activeForeground": "#f8fafc",
    "commandCenter.border": "#2b3a52",
    "commandCenter.inactiveForeground": "#94a3b8",

    "editorGroupHeader.tabsBackground": "#0f172a",
    "editorGroupHeader.noTabsBackground": "#0f172a",
    "editorGroupHeader.tabsBorder": "#2b3a52",

    "tab.activeBackground": "#0f172a",
    "tab.activeForeground": "#f8fafc",
    "tab.activeBorderTop": "#7c3aed",
    "tab.border": "#2b3a52",
    "tab.unfocusedActiveBackground": "#0f172a",
    "tab.inactiveBackground": "#1e293b",
    "tab.unfocusedInactiveBackground": "#1e293b",
    "tab.inactiveForeground": "#cbd5e1",
    "tab.hoverBackground": "#1f2937",

    "editor.background": "#0f172a",
    "editor.foreground": "#f1f5f9",
    "editor.lineHighlightBackground": "#1c2540",
    "editor.selectionBackground": "#43526dcc",
    "editor.inactiveSelectionBackground": "#2a385388",
    "editorCursor.foreground": "#f8fafc",
    "editorWhitespace.foreground": "#263248",
    "editorIndentGuide.background": "#233047",
    "editorIndentGuide.activeBackground": "#3a4a66",
    "editorLineNumber.foreground": "#556882",
    "editorLineNumber.activeForeground": "#e2e8f0",

    "editor.findMatchBackground": "#7c3aed55",
    "editor.findMatchBorder": "#a78bfa",
    "editor.findMatchHighlightBackground": "#7c3aed33",
    "editor.selectionHighlightBackground": "#7c3aed22",
    "editor.wordHighlightBackground": "#2563eb22",
    "editor.wordHighlightStrongBackground": "#2563eb33",
    "editorBracketMatch.background": "#33415566",
    "editorBracketMatch.border": "#a78bfa",

    "sideBar.background": "#1e293b",
    "sideBar.foreground": "#e2e8f0",
    "sideBar.border": "#2b3a52",
    "sideBarSectionHeader.background": "#253149",
    "sideBarSectionHeader.foreground": "#f8fafc",

    "panel.background": "#0f172a",
    "panel.border": "#2b3a52",
    "panelTitle.activeForeground": "#f8fafc",
    "panelTitle.inactiveForeground": "#94a3b8",

    "list.focusBackground": "#2a3650",
    "list.focusForeground": "#ffffff",
    "list.activeSelectionBackground": "#33455f",
    "list.activeSelectionForeground": "#f8fafc",
    "list.inactiveSelectionBackground": "#2a3649",
    "list.inactiveSelectionForeground": "#e2e8f0",
    "list.hoverBackground": "#1f2937",
    "list.hoverForeground": "#f8fafc",
    "list.highlightForeground": "#ffffff",

    "quickInput.background": "#0f172a",
    "quickInput.foreground": "#f1f5f9",
    "pickerGroup.border": "#2b3a52",
    "pickerGroup.foreground": "#cbd5e1",

    "dropdown.background": "#1e293b",
    "dropdown.foreground": "#f8fafc",
    "dropdown.border": "#2b3a52",

    "input.background": "#1e293b",
    "input.foreground": "#f8fafc",
    "input.border": "#2b3a52",
    "input.placeholderForeground": "#9aa7bd",

    "menu.background": "#0f172a",
    "menu.foreground": "#f8fafc",
    "menu.separatorBackground": "#2b3a52",
    "menubar.selectionBackground": "#1f2937",
    "menubar.selectionForeground": "#f8fafc",

    "activityBar.background": "#1e293b",
    "activityBar.foreground": "#f8fafc",
    "activityBarBadge.background": "#334155",
    "activityBarBadge.foreground": "#f8fafc",

    "statusBar.background": "#111827",
    "statusBar.foreground": "#f1f5f9",
    "statusBar.border": "#2b3a52",

    "textLink.foreground": "#60a5fa",
    "textLink.activeForeground": "#93c5fd",
    "focusBorder": "#7c3aed",
    "selection.background": "#43526dcc",

    "notification.background": "#1e293b",
    "notification.foreground": "#f8fafc",
    "notification.buttonBackground": "#334155",
    "notification.buttonForeground": "#f8fafc",
    "notification.infoBackground": "#1e293b",
    "notification.warningBackground": "#3a2f1b",
    "notification.errorBackground": "#3a2530",

    "editorWidget.background": "#0f172a",
    "editorWidget.foreground": "#f1f5f9",
    "peekViewTitle.background": "#0f172a",
    "peekViewEditor.background": "#0f172a",
    "peekViewResult.background": "#0f172a",
    "scrollbarSlider.background": "#48567099",
    "scrollbarSlider.hoverBackground": "#6c7a92bb",
    "scrollbarSlider.activeBackground": "#9fb0cacc",

    "button.background": "#334155",
    "button.hoverBackground": "#3b475b",
    "button.foreground": "#f8fafc",

    "badge.background": "#334155",
    "badge.foreground": "#f8fafc",

    "terminal.background": "#0f172a",
    "terminal.foreground": "#f1f5f9",
    "terminal.ansiBlack": "#111827",
    "terminal.ansiRed": "#ef4444",
    "terminal.ansiGreen": "#22c55e",
    "terminal.ansiYellow": "#eab308",
    "terminal.ansiBlue": "#60a5fa",
    "terminal.ansiMagenta": "#c084fc",
    "terminal.ansiCyan": "#38bdf8",
    "terminal.ansiWhite": "#e5e7eb",
    "terminal.ansiBrightBlack": "#374151",
    "terminal.ansiBrightRed": "#f87171",
    "terminal.ansiBrightGreen": "#4ade80",
    "terminal.ansiBrightYellow": "#fde047",
    "terminal.ansiBrightBlue": "#93c5fd",
    "terminal.ansiBrightMagenta": "#e9d5ff",
    "terminal.ansiBrightCyan": "#7dd3fc",
    "terminal.ansiBrightWhite": "#ffffff",

    "gitDecoration.addedResourceForeground": "#4ade80",
    "gitDecoration.modifiedResourceForeground": "#93c5fd",
    "gitDecoration.deletedResourceForeground": "#fca5a5",
    "gitDecoration.untrackedResourceForeground": "#4ade80",
    "gitDecoration.ignoredResourceForeground": "#64748b",
    "gitDecoration.conflictingResourceForeground": "#fbbf24"
  },
  "tokenColors": [
    { "scope": ["comment"], "settings": { "foreground": "#94a3b8", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"], "settings": { "foreground": "#c084fc", "fontStyle": "bold" } },
    { "scope": ["support.type", "entity.name.type", "entity.name.type.interface", "storage.type.type", "meta.type.annotation", "entity.name.namespace"], "settings": { "foreground": "#60a5fa" } },
    { "scope": ["string", "punctuation.definition.string"], "settings": { "foreground": "#4ade80" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#f472b6" } },
    { "scope": ["variable.other.constant", "support.constant", "constant.language"], "settings": { "foreground": "#38bdf8" } },
    { "scope": ["entity.name.function", "support.function", "meta.function-call"], "settings": { "foreground": "#f1f5f9" } },
    { "scope": ["variable.parameter", "meta.parameter"], "settings": { "foreground": "#e5e7eb" } },
    { "scope": ["entity.name.type.interface.ts", "entity.other.inherited-class"], "settings": { "foreground": "#60a5fa", "fontStyle": "bold" } },
    { "scope": ["entity.name.type.class", "meta.class", "support.class"], "settings": { "foreground": "#38bdf8" } },
    { "scope": ["meta.decorator", "storage.type.annotation", "punctuation.decorator"], "settings": { "foreground": "#fbbf24" } },
    { "scope": ["entity.name.tag", "support.class.component", "meta.tag.tsx"], "settings": { "foreground": "#fb923c" } },
    { "scope": ["entity.other.attribute-name", "meta.tag.attributes.tsx"], "settings": { "foreground": "#38bdf8" } }
  ],
  "semanticTokenColors": {
    "function": "#f1f5f9",
    "parameter": "#e5e7eb",
    "property": "#e5e7eb",
    "variable.readonly": "#e5e7eb",
    "type": "#60a5fa",
    "class": "#38bdf8",
    "enumMember": "#38bdf8",
    "keyword": "#c084fc",
    "string": "#4ade80",
    "number": "#f472b6",
    "comment": "#94a3b8"
  }
}
